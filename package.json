{
   "name": "bump-package",
   "description": "Bump package version based on keywords in head commit",
   "version": "2.5.12",
   "main": "lib/main.js",
   "repository": "https://github.com/jpb06/bump-package.git",
   "author": "jpb06 <jpb.06@outlook.com>",
   "license": "MIT",
   "engines": {
      "node": "20.x"
   },
   "scripts": {
      "cleanDist": "rimraf ./dist && rimraf ./lib",
      "build": "pnpm cleanDist && tsc --project tsconfig.prod.json && ncc build",
      "type-check": "tsc --noEmit",
      "lint": "eslint ./src/**/*.ts",
      "test": "jest",
      "test-dev": "jest --watchAll --coverage",
      "test-ci": "jest --ci --coverage",
      "sync-icons": "pnpm generateReadmeIcons -h 50"
   },
   "dependencies": {
      "@actions/core": "^1.10.0",
      "@actions/exec": "^1.1.1",
      "@actions/github": "^6.0.0"
   },
   "devDependencies": {
      "@commitlint/cli": "^18.4.3",
      "@commitlint/config-conventional": "^18.4.3",
      "@swc/core": "^1.3.60",
      "@swc/jest": "^0.2.26",
      "@types/jest": "^29.5.1",
      "@types/node": "^20.2.5",
      "@typescript-eslint/eslint-plugin": "^6.16.0",
      "@typescript-eslint/parser": "^6.16.0",
      "@vercel/ncc": "^0.38.1",
      "eslint": "^8.41.0",
      "eslint-config-prettier": "^9.1.0",
      "eslint-plugin-import": "^2.27.5",
      "eslint-plugin-jest": "^27.2.1",
      "eslint-plugin-prettier": "^5.1.2",
      "jest": "^29.5.0",
      "jest-watch-typeahead": "^2.2.2",
      "prettier": "^3.1.1",
      "readme-package-icons": "^1.1.8",
      "rimraf": "^5.0.1",
      "typescript": "^5.0.4"
   }
}
