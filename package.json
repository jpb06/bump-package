{
   "name": "bump-package",
   "description": "Bump package version based on keywords in head commit",
   "version": "2.5.12",
   "main": "lib/main.js",
   "repository": {
      "type": "git",
      "url": "git+https://github.com/jpb06/bump-package.git"
   },
   "author": "jpb06 <jpb.06@outlook.com>",
   "license": "MIT",
   "engines": {
      "node": "20.x"
   },
   "scripts": {
      "cleanDist": "rimraf ./dist && rimraf ./lib",
      "build": "pnpm cleanDist && tsc --project tsconfig.prod.json && ncc build",
      "type-check": "tsc --noEmit",
      "lint": "eslint ./src/**/*.ts",
      "test": "vitest",
      "test-ci": "vitest --coverage --run",
      "test-dev": "vitest --coverage",
      "test-coverage": "vitest run --coverage",
      "sync-icons": "pnpm generateReadmeIcons -h 50"
   },
   "dependencies": {
      "@actions/core": "^1.10.0",
      "@actions/exec": "^1.1.1",
      "@actions/github": "^6.0.0"
   },
   "devDependencies": {
      "@commitlint/cli": "^18.6.0",
      "@commitlint/config-conventional": "^18.6.0",
      "@stylistic/eslint-plugin": "^1.6.1",
      "@types/eslint": "^8.56.2",
      "@types/node": "^20.11.17",
      "@typescript-eslint/eslint-plugin": "^6.21.0",
      "@typescript-eslint/parser": "^6.21.0",
      "@vercel/ncc": "^0.38.1",
      "@vitest/coverage-v8": "^1.2.2",
      "eslint": "^8.41.0",
      "eslint-config-prettier": "^9.1.0",
      "eslint-import-resolver-typescript": "^3.6.1",
      "eslint-plugin-import": "^2.27.5",
      "eslint-plugin-markdown": "^3.0.1",
      "eslint-plugin-prettier": "^5.1.3",
      "eslint-plugin-vitest": "^0.3.22",
      "prettier": "^3.2.5",
      "readme-package-icons": "^1.1.8",
      "rimraf": "^5.0.1",
      "typescript": "^5.0.4",
      "vitest": "^1.2.2"
   }
}
