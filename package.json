{
   "name": "bump-package",
   "description": "Bump package version based on keywords in head commit",
   "version": "2.5.8",
   "main": "lib/main.js",
   "repository": "https://github.com/jpb06/bump-package.git",
   "author": "jpb06 <jpb.06@outlook.com>",
   "license": "MIT",
   "scripts": {
      "cleanDist": "rimraf ./dist && rimraf ./lib",
      "build": "pnpm cleanDist && tsc --project tsconfig.prod.json && ncc build",
      "type-check": "tsc --noEmit",
      "lint": "eslint ./src/**/*.ts",
      "test": "jest",
      "test-dev": "jest --watchAll --coverage",
      "test-ci": "jest --ci --coverage",
      "sync-icons": "pnpm generateReadmeIcons -h 50"
   },
   "dependencies": {
      "@actions/core": "^1.10.0",
      "@actions/exec": "^1.1.1",
      "@actions/github": "^5.1.1"
   },
   "devDependencies": {
      "@commitlint/cli": "^17.4.2",
      "@commitlint/config-conventional": "^17.4.2",
      "@swc/core": "^1.3.29",
      "@swc/jest": "^0.2.24",
      "@types/jest": "^29.4.0",
      "@types/node": "^18.11.18",
      "@typescript-eslint/eslint-plugin": "^5.49.0",
      "@typescript-eslint/parser": "^5.49.0",
      "@vercel/ncc": "^0.36.1",
      "eslint": "^8.32.0",
      "eslint-config-prettier": "^8.6.0",
      "eslint-plugin-import": "^2.27.5",
      "eslint-plugin-jest": "^27.2.1",
      "eslint-plugin-prettier": "^4.2.1",
      "jest": "^29.4.1",
      "jest-mock": "^29.4.1",
      "jest-watch-typeahead": "^2.2.2",
      "prettier": "^2.8.3",
      "readme-package-icons": "^1.1.8",
      "rimraf": "^4.1.2",
      "typescript": "^4.9.4"
   }
}
